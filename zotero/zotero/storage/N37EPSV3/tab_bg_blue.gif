<script type="text/javascript">
    $(document).ready(function ()
    {
        CheckCode('');
    });

    function CheckCode(code)
    {
        var url = '';
        var resultUrl = '';
        switch(code)
        {
            case '400':
                {
                    if (url.indexOf('t.hujiang.com') >= 0)
                    {
                        resultUrl = 'http://t.hujiang.com';
                    }
                    else if (url.indexOf('/diary') >= 0)
                    {
                        resultUrl = 'http://bulo.hujiang.com/app/diary/';
                    }
                    else if (url.indexOf('/home') >= 0)
                    {
                        resultUrl = 'http://bulo.hujiang.com/home/';
                    }
                }
                break;
            case '201':
                resultUrl = 'http://pass.hujiang.com/signup/';
                break;
            case '202':
                resultUrl = 'http://bulo.hujiang.com/home/';
                break;
            case '504':
                resultUrl = 'http://bulo.hujiang.com/group/';
                break;
            case '999':
                resultUrl = 'http://bulo.hujiang.com/home/';
                break;
            case '40314':
                resultUrl = 'http://bulo.hujiang.com/home/';
                break;
            default:
                resultUrl = '';
        }
        //跳转
        jump(10, resultUrl);
    }

    //自动跳转
    function jump(count, toUrl)
    {
        if ((count == 0) || (toUrl == ''))
        {
            $('#AutoOpenInfo').hide();
        }
        else {
            $('#AutoOpenInfo').show();
            window.setInterval(function () {
                count--;
                if (count > 0) {
                    $('#AutoOpenTime').attr('innerHTML', count);
                } else {
                    location.href = toUrl;
                }
            }, 1000);
        }
    }
</script>
